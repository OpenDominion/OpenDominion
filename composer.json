{
  "type": "project",
  "require": {
    "php": "^8.2",
    "ext-bcmath": "*",
    "ext-curl": "*",
    "ext-fileinfo": "*",
    "ext-gd": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pdo_mysql": "*",
    "atrox/haikunator": "^1.3",
    "beyondcode/laravel-self-diagnosis": "^3.0",
    "bugsnag/bugsnag-laravel": "~2.0",
    "graham-campbell/markdown": "^16.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "intervention/image": "~2.4",
    "jenssegers/agent": "^2.6",
    "laravel/framework": "^12.0",
    "laravel/tinker": "^2.9",
    "laravel/ui": "^4.5",
    "league/flysystem-aws-s3-v3": "^3.0",
    "spatie/db-dumper": "^3.0",
    "spatie/laravel-backup": "^9.0",
    "spatie/laravel-honeypot": "^4.0",
    "spatie/laravel-ignition": "^2.0",
    "spatie/laravel-permission": "^6.0",
    "symfony/yaml": "^6.0",
    "creativeorange/gravatar": "^1.0",
    "webpatser/laravel-uuid": "^4.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.16",
    "barryvdh/laravel-ide-helper": "^3.0",
    "beyondcode/laravel-dump-server": "^2.0",
    "deployer/deployer": "^7.2",
    "fakerphp/faker": "^1.20",
    "laravel/browser-kit-testing": "^7.0",
    "laravel/sail": "^1.19",
    "mockery/mockery": "^1.5",
    "nunomaduro/collision": "^8.0",
    "phpunit/phpunit": "^11.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "OpenDominion\\": "src/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "classmap": [
      "app/database/factories",
      "app/database/seeders"
    ],
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "OpenDominion\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:models -N",
      "php artisan ide-helper:meta"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "php artisan package:discover"
    ]
  }
}
