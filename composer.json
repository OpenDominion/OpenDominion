{
	"name": "wavehack/opendominion",
	"license": "MIT",
	"type": "project",
	"authors": [
		{
			"name": "WaveHack",
			"homepage": "https://github.com/WaveHack"
		}
	],
	"require": {
		"laravel/framework": "~5.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
		"barryvdh/laravel-ide-helper": "~1.11",
		"barryvdh/laravel-debugbar": "~2.0",
		"behat/behat": "~3.0@dev",
		"behat/mink": "~1.7@dev",
		"behat/mink-extension": "~2.0@dev",
		"laracasts/behat-laravel-extension": "dev-master"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"OpenDominion\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
